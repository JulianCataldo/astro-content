// const props = Astro.props;
import { Tweet } from '@astro-community/astro-embed-twitter';

let url: string = '';
if (Astro.slots.has('default')) {
  const html = await Astro.slots.render('default');

  [, url] = html.match(/href="(.*)"/i);
}
console.log({ url });
---

<Tweet class="tweet" id={url} />
